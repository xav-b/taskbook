{
  "name": "taskbook",
  "productName": "Taskbook",
  "version": "0.10.0",
  "description": "Tasks, boards & notes for the command-line habitat",
  "repository": "klaussinani/taskbook",
  "license": "MIT",
  "author": {
    "name": "Klaus Sinani",
    "email": "klaussinani@gmail.com",
    "url": "https://klaussinani.github.io"
  },
  "bin": {
    "tb": "dist/src/interfaces/cli.js"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "task",
    "board",
    "note",
    "todo",
    "notebook",
    "command",
    "line",
    "console",
    "cli",
    "app"
  ],
  "configuration": {
    "default": {
      "displayCompleteTasks": true,
      "displayProgressOverview": true,
      "displayWarnings": true,
      "enableCopyID": false,
      "eventBoard": "calendar"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "test": "vitest"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "clipboardy": "2.3.0",
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "enquirer": "^2.4.1",
    "env-paths": "^3.0.0",
    "marked": "^10.0.0",
    "marked-terminal": "^6.1.0",
    "meow": "^12.1.1",
    "nanoid": "3",
    "signale": "1.4.0",
    "tmp": "^0.2.1",
    "tracer": "^1.3.0",
    "typescript": "^5.2.2",
    "update-notifier": "5.0.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/marked-terminal": "^6.0.1",
    "@types/node": "^20.9.1",
    "@types/signale": "^1.4.7",
    "@types/tmp": "^0.2.6",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-pretty": "^6.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "semver": "^7.5.4",
    "vitest": "^0.34.6"
  },
  "xo": {
    "space": 2
  },
  "packageManager": "yarn@4.0.2"
}
